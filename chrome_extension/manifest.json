{
  "manifest_version": 2,
  "name": "Chaddon",
  "description": "This extension allows us to connect with global users.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Chaddon"
},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "icons": { 
    "19": "icon19.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
